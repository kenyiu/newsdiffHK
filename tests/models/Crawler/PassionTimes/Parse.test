<?php


class TestCase_Crawler_PassionTimes_Parse_19750 extends UnitTestCase
{

    function __construct()
    {
        $this->label = 'Crawler_PassionTimes::parse (19750.html)';
    }

    function testParse()
    {
        $body = file_get_contents(__DIR__ . '/data/19750.html');
        $result = Crawler_PassionTimes::parse($body);
        $reference = json_decode(file_get_contents(__DIR__ . '/data/19750.json'));

        $this->assertEqual(
          $result->title,
          $reference->title,
          'Parsed Page Title'
        );
        $this->assertEqual(
          $result->body,
          $reference->body,
          'Parsed Page Body'
        );
    }

}

